# AILinux Repo Docker Environment
# IMPORTANT: Never commit this file to version control!
# Copy this file to .env and fill in your values

# Timezone
TZ=Europe/Berlin

# Cloudflare API (for cache purging)
CF_GLOBAL_API_KEY=your_cloudflare_global_api_key_here
CF_EMAIL=admin@your-domain.com
CF_ZONE_ID=your_cloudflare_zone_id_here

# GPG Signing Key ID
SIGNING_KEY_ID=YOUR_GPG_KEY_ID_HERE

# Mirror Configuration
BASE_URL=https://repo.your-domain.com:8443
PUBLIC_PATH=mirror
